{
  "name": "larsenwork",
  "version": "1.0.0",
  "description": "My personal webiste",
  "author": "larsenwork <andreas@larsenwork.com>",
  "private": true,
  "scripts": {
    "dev": "cross-env HOST=0.0.0.0 PORT=3333 nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "deploy": "push-dir --dir=dist --branch=master --cleanup",
    "live": "npm run generate && npm run deploy",
    "lint": "eslint --ext .js,.vue src test --ignore-path .gitignore .",
    "lint-fix": "eslint --ext .js,.vue src test --fix --ignore-path .gitignore .",
    "eslint-check": "eslint --print-config .eslintrc.js | eslint-config-prettier-check",
    "precommit": "npm run eslint-check && npm run lint"
  },
  "browserslist": [
    "> 5%",
    "last 1 versions"
  ],
  "dependencies": {
    "nuxt": "^1.4.0"
  },
  "devDependencies": {
    "autoprefixer": "^7.2.6",
    "babel-eslint": "^7.2.3",
    "chroma-js": "^1.3.7",
    "cross-env": "^5.1.4",
    "easing-coordinates": "^1.0.4",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-html": "^3.1.1",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-standard": "^3.1.0",
    "eslint-plugin-vue": "^4.5.0",
    "husky": "^0.14.3",
    "postcss-custom-media": "^6.0.0",
    "postcss-easing-gradients": "^2.0.1",
    "postcss-import": "^11.1.0",
    "postcss-nesting": "^4.2.1",
    "prettier": "^1.12.1",
    "push-dir": "^0.4.1",
    "throttleit": "^1.0.0",
    "vue-slider-component": "^2.6.2",
    "webpack-bundle-analyzer": "^2.11.1"
  }
}
